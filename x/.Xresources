Xcursor.theme: xcursor-breeze
Xcursor.size:                     0

! URxvt.font:           9x15,xft:TerminessTTFNerdFontMono
! URxvt.font: 		xft:Inconsolata:bold:size=11
URxvt.font:		xft:DejaVu Sans Mono for Powerline:pixelsize=12
